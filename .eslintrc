{
    "extends": "standard",
    "rules": {
        "semi": [2, "never"],
        "indent": [2],
        "no-return-await": 0,
        "space-before-function-paren": [
            2,
            {
                "named": "never",
                "anonymous": "never",
                "asyncArrow": "always"
            }
        ],
        "template-curly-spacing": [2, "always"]
    }
}
